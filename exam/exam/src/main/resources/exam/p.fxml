<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="351.0" prefWidth="307.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exam.PrimaryController">
   <children>
      <Label text="Выберите файл с числами">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <TextField fx:id="fileName" />
      <Button fx:id="selectFile" text="Выбрать" onAction="#selectFile"/>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" prefHeight="25.0" prefWidth="261.0" text="Вычислить среднее арифмитическое" />
      <Label prefHeight="44.0" prefWidth="262.0" text="Label" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
